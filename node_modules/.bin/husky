#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/前端/面试题/算法/algorithm/node_modules/.pnpm/husky@7.0.4/node_modules/husky/lib/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/.pnpm/husky@7.0.4/node_modules/husky/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/.pnpm/husky@7.0.4/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/.pnpm/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules:/mnt/d/前端/面试题/算法/node_modules:/mnt/d/前端/面试题/node_modules:/mnt/d/前端/node_modules:/mnt/d/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/husky/lib/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/husky/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/前端/面试题/算法/algorithm/node_modules/.pnpm/husky@7.0.4/node_modules/husky/lib/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/.pnpm/husky@7.0.4/node_modules/husky/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/.pnpm/husky@7.0.4/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/.pnpm/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules:/mnt/d/前端/面试题/算法/node_modules:/mnt/d/前端/面试题/node_modules:/mnt/d/前端/node_modules:/mnt/d/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/husky/lib/node_modules:/mnt/d/前端/面试题/算法/algorithm/node_modules/husky/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/lib/bin.js" "$@"
else
  exec node  "$basedir/../husky/lib/bin.js" "$@"
fi
